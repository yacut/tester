{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "flowtype"
    ],
    "env": {
      "atomtest": true,
      "es6": true,
      "jasmine": true,
      "node": true
    },
    "globals": {
      "atom": false,
      "document": true,
      "window": true
    },
    "rules": {
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "no-restricted-syntax": 0,
      "strict": 0,
      "class-methods-use-this": 0,
      "no-console": 0,
      "no-param-reassign": 0,
      "no-useless-escape": 0,
      "prefer-rest-params": 0,
      "max-len": 0,
      "react/jsx-filename-extension": 0,
      "react/sort-comp": 0
    }
}
